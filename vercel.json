{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "turbo run build --filter=@nimble/web",
  "framework": "nextjs",
  "outputDirectory": "apps/web/out",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.nimble.bitnook.cc/:path*"
    }
  ]
}
